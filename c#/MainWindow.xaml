<Window x:Class="Quest_Song_Exporter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Quest_Song_Exporter"
        mc:Ignorable="d"
        Title="Quest Song Exporter" Height="450" Width="500" Background="#FF282828" >
    <Grid>
        <Button Click="Button_Click" Content="Choose Song Folder" HorizontalAlignment="Left" Margin="290,200,0,0" VerticalAlignment="Top" Width="200" Height="33" RenderTransformOrigin="-0.858,0.891" Background="#FF005654" Foreground="#FFF3F3F3" BorderBrush="#FFB94747"/>
        <TextBox IsReadOnly="True" x:Name="txtbox" HorizontalAlignment="Left" Height="400" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="275" Background="#FF494949" Foreground="#FFA6A6A6" BorderBrush="#FFD30000"/>


    </Grid>
</Window>
